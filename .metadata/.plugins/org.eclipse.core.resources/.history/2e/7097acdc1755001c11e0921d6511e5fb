package magazineStand;

public class Magazine {
	private String title;
	private double cost;
	private int numInStock;
	private int counter=numInStock;
	
	public Magazine() {
		this.cost=3.50;
		this.numInStock=10;
	}
	private boolean checkStock() {
		if(this.counter>0) {
			return true;
			this.counter--;
		}else {
			return false;
		}
   }
	
}
